<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace= "org.itcgae.siga.db.services.com.mappers.ConConsultasExtendsMapper">

	<resultMap type="hashmap" id="mapa">
	</resultMap>
	<sql id="select">
		SELECT ${query.selectValue}
	</sql>
	<sql id="from">
		FROM ${query.fromValue}
	</sql>
	<sql id="innerJoin">
		INNER JOIN ${query.innerJoinValue}
	</sql>
	<sql id="join">
		JOIN ${query.joinValue}
	</sql>
	<sql id="leftJoin">
		LEFT JOIN ${query.leftJoinValue}
	</sql>
	<sql id="outerJoin">
		OUTER JOIN ${query.outerJoinValue}S
	</sql>
	<sql id="where">
		WHERE ${query.whereValue}
	</sql>
	<sql id="orderBy">
		ORDER BY ${query.orderByValue}
	</sql>
	<sql id="groupBy">
		GROUP BY ${query.groupByValue}
	</sql>
	<sql id="having">
		HAVING ${query.havingValue}
	</sql>
	<select id="ejecutarConsulta" parameterType="hashmap" resultMap="mapa">
	
		<include refid="select"></include>
		
		<include refid="from"></include>
		

    	<if test="query.innerJoinValue != '' and query.innerJoinValue != null">
	    	<include refid="innerJoin">
	    	</include>
    	</if>
    	
    	<if test="query.joinValue != '' and query.joinValue != null">
    		<include refid="join">
    		</include>
    	</if>
    	
    	<if test="query.leftJoinValue != '' and query.leftJoinValue != null">
	    	<include refid="leftJoin">
	    	</include>
    	</if>
    	
    	<if test="query.outerJoinValue != '' and query.outerJoinValue != null">
	    	<include refid="outerJoin">
	    	</include>
  		</if>
  		
  		<if test="query.wherevalue != '' and query.wherevalue != null">
	    	<include refid="where">
	    	</include>
    	</if>
    	
  		<if test="query.orderByValue != '' and query.orderByValue != null">
	    	<include refid="orderBy">
	    	</include>
  		</if>
  		
  		<if test="query.groupByValue != '' and query.groupByValue != null">
	    	<include refid="groupBy">
	    	</include>
  		</if>
  		
  		<if test="query.havingValue != '' and query.havingValue != null">
	    	<include refid="having">
	    	</include>
    	</if>

  		
	</select>
</mapper>